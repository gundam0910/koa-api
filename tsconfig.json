{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es2016",
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "./",
        "rootDir": "src",
        "allowJs": true,
        "resolveJsonModule": true,
        "noEmit": true,
        "strict": false
    },
    "lib": ["es2015"],
    "include": ["./src/**/*", "./sequelize/**/*.ts", "./sequelize/**/*.js"],
    "exclude": [
        "node_modules",
        "sequelize"
    ]
}
